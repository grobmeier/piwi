<?xml version='1.0' standalone='yes'?>
<!DOCTYPE generators SYSTEM "dtd/generators.dtd">
<generators>
	<generator  id="default-content"
				class="SQLLiteContentGenerator">
		<connection>
			default-connection
		</connection>
		<sql>
			SELECT * FROM content where ID='${id}'
		</sql>
	</generator>
</generators>
