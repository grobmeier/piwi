<?xml version='1.0' standalone='yes'?>
<connectors>
	<connector id="sqlite2Connector" class="SQLite2Connector">
		<file>custom/content/db/sqlite2_sample.sqlite2</file>
		<mode>0666</mode>
	</connector>
	<connector id="sqlite3Connector" class="SQLite3Connector">
		<file>custom/content/db/sqlite3_sample.sqlite3</file>
	</connector>
	<connector id="mySQLConnector" class="MySQLConnector">
		<server>127.0.0.1:3306</server>
		<database>databasename</database>
		<username>username</username>
		<password>password</password>
	</connector>
</connectors>