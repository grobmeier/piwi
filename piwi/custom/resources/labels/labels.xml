<?xml version="1.0" encoding="UTF-8"?>
<labels xmlns="http://piwi.googlecode.com/xsd/labels" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://piwi.googlecode.com/xsd/labels ../../../resources/xsd/labels.xsd">
	<language region="default">
	   <!-- LoginProcessor -->
	   <label key="LOGIN_SUCCESS" value="Login successful." />
	   <label key="LOGIN_FAILED" value="Login failed." />

      <!-- ValidationProcessor -->
      <label key="GENDER" value="Geschlecht" />
      <label key="EMAIL" value="Email" />
      <label key="NUMBER" value="Nummer" />
      <label key="NEWSLETTER" value="Newsletter" />
      
      <!-- WizardProcessor -->
      <label key="BIRTHDAY" value="Birthday" />
      <label key="COMMENT" value="Comment" />
      
      <!-- DataGridGenerator -->
      <label key="NAME" value="Name" />
      <label key="ADRESS" value="Adress" />
      <label key="CITY" value="City" />
      <label key="PHONE" value="Phone" />
         
      <!-- SiteMap -->
      <label key="CONTENTS_OF_THIS_WEBSITE" value="Contents of this Website" />
	</language>
	<language region="de">
	   <!-- LoginProcessor -->
	   <label key="LOGIN_SUCCESS" value="Login erfolgreich." />
      <label key="LOGIN_FAILED" value="Login fehlgeschlagen." />

      <!-- ValidationProcessor -->
      <label key="GENDER" value="Geschlecht" />
      <label key="EMAIL" value="Email" />
      <label key="NUMBER" value="Nummer" />
      <label key="NEWSLETTER" value="Newsletter" />
      
      <!-- WizardProcessor -->
      <label key="BIRTHDAY" value="Geburtstag" />
      <label key="COMMENT" value="Kommentar" />
      
      <!-- DataGridGenerator -->
      <label key="NAME" value="Name" />
      <label key="ADRESS" value="Adresse" />
      <label key="CITY" value="Stadt" />
      <label key="PHONE" value="Telefon" />      
      
	   <!-- SiteMap -->
	   <label key="CONTENTS_OF_THIS_WEBSITE" value="Inhalte dieser Webseite" />
	</language>
</labels>